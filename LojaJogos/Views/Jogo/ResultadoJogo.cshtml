            @Html.DisplayFor(model => model.AnoLanc)
        </div>

        <div class="editor-label">
            <b>@Html.DisplayNameFor(model => model.Sinopse):</b>
        </div>
        <div class="editor-field">
            @Html.DisplayFor(model => model.Sinopse)
        </div>
    </fieldset>
    <br />
